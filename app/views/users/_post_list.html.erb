<!-- 投稿一覧 -->
<%= paginate post_list %>
<% if post_list.present? %>
  <div class="post-list-box">
    <% post_list.each do |post| %>
      <div class="post-list-frame">
        <%= link_to post_path(post), class: "post-list-img" do %>
          <%= image_tag post.photos.first.image.url(:medium) %>
        <% end %>
      </div>
    <% end %>
  </div>
<% else %>
  <%= render 'users/post_list_none' %>
<% end %>
