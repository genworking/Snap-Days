<!-- 保存済み一覧 -->
<% if favorite_list.present? %>
  <div class="post-list-box">
    <% favorite_list.each do |post| %>
      <a href="<%= post_path(post) %>" class="post-list-frame">
        <%= image_tag post.photos.first.image.url(:medium), class: "post-list-img" %>
      </a>
    <% end %>
  </div>
<% else %>
  <a>お気に入り登録はまだありません</a>
<% end %>
